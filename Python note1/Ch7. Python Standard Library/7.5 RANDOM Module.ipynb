   "source": [
    "import random\n",
    "\n",
    "applicants = ['영희','철수','재석','가희','민석','오성','길동','개똥']\n",
    "K = 2\n",
    "\n",
    "winners = random.sample(applicants, K)\n",
    "print(winners)\n",
    "\n",
    "random.shuffle(applicants)\n",
    "winners1 = applicants[0:K]\n",
    "print(winners1)"
   ]
